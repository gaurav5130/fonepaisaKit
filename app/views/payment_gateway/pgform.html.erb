<%= form_tag (@form_submit) do %>
	<%= hidden_field_tag :msg_name, @msg_name  %>
	<%= hidden_field_tag :id, @id  %>
	<%= hidden_field_tag :merchant_id, @merchant_id  %>
	<%= hidden_field_tag :merchant_display, @merchant_display %>
	<%= hidden_field_tag :invoice_amt, @invoice_amt  %>
	<%= hidden_field_tag :amount, @amount  %>
	<%= hidden_field_tag :email, @email  %>
	<%= hidden_field_tag :mobile_no, @mobile_no %>
	<%= hidden_field_tag :callback_url, @callback_url  %>
	<%= hidden_field_tag :callback_failure_url, @callback_failure_url  %>
	<%= hidden_field_tag :invoice, @invoice  %>
	<%= hidden_field_tag :sign, @sign %>
	<%= submit_tag %>
<% end %>